{
	"name": "legoFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "colors",
						"type": "DatasetReference"
					},
					"name": "color"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "NoNanValues"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as string,",
				"          name as string,",
				"          rgb as string,",
				"          is_trans as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> color",
				"color filter(ERROR_FUNCTION('')) ~> NoNanValues"
			]
		}
	}
}